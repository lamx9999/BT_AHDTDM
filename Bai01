#include <stdio.h>

// Hàm in ra các số nguyên có 2 chữ số và là bội của 7
void printMultiplesOfSeven() {
    for (int i = 10; i < 100; i++) {
        if (i % 7 == 0) {
            printf("%d\n", i);
        }
    }
}

int main() {
    printf("Cac so nguyen co 2 chu so va la boi cua 7:\n");
    printMultiplesOfSeven();
    return 0;
}
